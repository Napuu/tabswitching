{
    "name": "tabswitching",
    "version": "1.0",
    "description": "tabswitching",
    "background": {
        "scripts": ["background.js"],
        "persistent": true 
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        } 
    ],
    "permissions": [
        "tabs",
        "storage"
    ], 
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true 
    },
    "manifest_version": 2
}
